{{#*inline "content"}}
    {{#if user}}
    <pre>
        id: {{user.id}}
        username: {{user.username}}
        name: {{user.name}}
        email: {{user.email}}
        avatar_url: {{user.avatar_url}}
        gravatar_id: {{user.gravatar_id}}
        github_id: {{user.github_id}}
        role: {{user.role}}
        created_at: {{user.created_at}}
        updated_at: {{user.updated_at}}
    </pre>
    {{/if}}
{{/inline}}
{{~> layouts/app_layout sub_title="Home" ~}}
